    "vue-i18n": "^8.0.0",
    "vue-router": "^2.8.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "vue2-dropzone": "^3.5.2",
    "vuelidate": "^0.7.4",
    "vuex": "^2.5.0",
    "vuex-router-sync": "^4.3.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
